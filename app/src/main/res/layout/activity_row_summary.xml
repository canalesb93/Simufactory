<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"

    android:weightSum="2"
    android:orientation="vertical"
    android:columnCount="6"
    android:rowCount="11"
    android:columnOrderPreserved="true">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Team Name"
        android:id="@+id/tvTeamName"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Money Spent"
        android:id="@+id/textView3"
        android:layout_below="@+id/tvTeamName"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="33dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ms value"
        android:id="@+id/tvMoneySpent"
        android:layout_alignBottom="@+id/textView3"
        android:layout_toRightOf="@+id/tvTeamName"
        android:layout_toEndOf="@+id/tvTeamName" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Money Earned"
        android:id="@+id/textView5"
        android:layout_marginTop="39dp"
        android:layout_below="@+id/textView3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="me value"
        android:id="@+id/tvMoneyEarned"
        android:layout_alignTop="@+id/textView5"
        android:layout_alignLeft="@+id/tvMoneySpent"
        android:layout_alignStart="@+id/tvMoneySpent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Produced Products"
        android:id="@+id/textView7"
        android:layout_marginTop="39dp"
        android:layout_below="@+id/textView5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="A"
        android:id="@+id/textView19"
        android:layout_alignBottom="@+id/textView7"
        android:layout_toRightOf="@+id/tvTeamName"
        android:layout_toEndOf="@+id/tvTeamName" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="B"
        android:id="@+id/textView20"
        android:layout_below="@+id/textView19"
        android:layout_alignLeft="@+id/textView19"
        android:layout_alignStart="@+id/textView19" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="C"
        android:id="@+id/textView21"
        android:layout_below="@+id/textView20"
        android:layout_toRightOf="@+id/tvTeamName"
        android:layout_toEndOf="@+id/tvTeamName" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="valueA"
        android:id="@+id/tvProdA"
        android:layout_alignTop="@+id/textView19"
        android:layout_toRightOf="@+id/tvMoneyEarned"
        android:layout_toEndOf="@+id/tvMoneyEarned" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="valueB"
        android:id="@+id/tvProdB"
        android:layout_below="@+id/tvProdA"
        android:layout_toRightOf="@+id/tvMoneyEarned"
        android:layout_toEndOf="@+id/tvMoneyEarned" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="valueC"
        android:id="@+id/tvProdC"
        android:layout_below="@+id/tvProdB"
        android:layout_toRightOf="@+id/tvMoneyEarned"
        android:layout_toEndOf="@+id/tvMoneyEarned" />
</RelativeLayout>
